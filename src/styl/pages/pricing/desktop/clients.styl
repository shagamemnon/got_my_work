section#clients
  background-color: #f9f9f9
  position: relative
  height: 440px
  padding-top: 120px
  border-top: 3px solid #ddd

  h2
    header2()
    text-align: center
    padding-bottom: 60px

  .clients-table
    height: auto
    padding: 35px 0 18px
    max-width: 960px
    margin: auto
    display: block
    text-align: center

    .tile
      col(1/5)
      clearfix()

      img
        height: 62px

      .ganesh

        img
          height: 50px
          padding-bottom: 12px

      p
        padding-top: 30px
        text-align: center
        font-size: 14px