section.signup-header
  .container
    .signup.company
      h2 Company
      form(method="post")
        em Enter Company Info
        input( name="email", type="text", placeholder="Company Name" )
        input( name="email", type="text", placeholder="Full Name" )
        input( name="name", type="text", placeholder="Phone" )
        em Create Account
        input( name="email", type="text", placeholder="Email" )
        input( name="name", type="text", placeholder="Create Password" )
        input( name="name", type="text", placeholder="Confirm Password" )
        input( type="text", name="_gotcha", style="display:none")
      .submit-button
        button Go